  <meta property="fb:admins" content="1326121540"/>

  <!--Twitter Meta tags-->
  <meta name="twitter:card" content="summary"/>
  <meta name="twitter:url" content="http://www.itsplatano.com"/>
  <meta name="twitter:title" content="Plat Ramos - Writing code by day, making beats by night"/>
  <meta name="twitter:description" content="Hailing from Queens, NY Plat is a producer with influences as diverse as his borough. Quality and variety are staples in his music and he plans to continue that"/>
  <meta name="twitter:image" content="http://www.itsplatano.com/assets/images/plat-ramos-photo.jpg"/>
  <meta name="twitter:creator" content="@PlatRamos">
  <meta name="twitter:site" content="@PlatRamos">

  <!-- Schema.org markup for Google+ -->
  <meta itemprop="name" content="Plat Ramos - Writing code by day, making beats by night">
